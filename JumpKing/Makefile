compile:
	g++ -Iinclude -Iinclude/sdl -Iinclude/headers -Llib -o JumpKing src/*.cpp -lmingw32 -lSDL2main -lSDL2 -lSDL2_image -lSDL2_mixer

run:
	g++ -Iinclude -Iinclude/sdl -Iinclude/headers -Llib -o JumpKing src/*.cpp -lmingw32 -lSDL2main -lSDL2 -lSDL2_image -lSDL2_mixer
	./JumpKing.exe

run-only:
	./JumpKing.exe